@{
    Layout = null;
}
@using BIDV.Common
@model List<BIDV.Model.bidv__card>
<ul>
    @for (int i = 0; i < Model.Count; i++)
    {
        <li class='@(i==0?"active":"")'><a href="/CardService/Details/@Model[i].id"><img width="105" height="65" src="/Content/FrontEnd/_img_server/card/@HelperDateTime.DateTimeToyyyymmdd(Model[i].created.Value)/size280/@Model[i].image" /></a></li>
    }
</ul>